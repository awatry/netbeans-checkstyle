<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">

<filesystem>
    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="cz-sickboy-netbeans-checkstyle-options-CheckstyleOptions.instance">
                <attr name="instanceClass" stringvalue="cz.sickboy.netbeans.checkstyle.options.CheckstyleOptions"/>
            </file>
        </folder>
    </folder> 
    
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="cz-sickboy-netbeans-checkstyle-resources-checkstyle-annotation.xml"
                url="nbresloc:/cz/sickboy/netbeans/checkstyle/resources/checkstyle-annotation.xml"/>
        </folder>
    </folder>

    <folder name="TaskList">
        <folder name="Groups">
            <file name="cz-sickboy-netbeans-checkstyle-tasklist-CheckstyleTaskGroup.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                
                <attr name="localizingBundle" stringvalue="cz.sickboy.netbeans.checkstyle.tasklist.Bundle"/>
                <attr name="groupName" stringvalue="cz-sickboy-netbeans-checkstyle-Task"/>
                <attr name="diplayNameKey" stringvalue="CheckstyleTaskGroup.label"/>
                <attr name="descriptionKey" stringvalue="CheckstyleTaskGroup.hint"/>
                <attr name="iconKey" stringvalue="CheckstyleTaskGroup.icon"/>
                <attr name="position" intvalue="250"/>
            </file>            
        </folder>
        
        <folder name="Scanners">
            <file name="cz-sickboy-netbeans-checkstyle-tasklist-CheckstyleTaskScanner.instance">
                <attr name="instanceClass" stringvalue="cz.sickboy.netbeans.checkstyle.tasklist.CheckstyleTaskScanner"/>
            </file>         
        </folder>
    </folder> 
</filesystem>
